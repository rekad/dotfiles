-- auto formatting of output
\x auto

-- set linestyles
\pset linestyle unicode
\pset border 2

-- disable pager
\pset pager off

-- time every query
\timing

-- configure prompt
\set PROMPT1 '%M:%> %n@%/%R%#%x '

